
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @db.VarChar(36)  @id @default(uuid())
  name  String
  email String @unique
  address String
  number Int
  phone Int
  document Int
  shopkeeper Boolean
}